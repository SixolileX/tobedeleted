<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Inn_TuneProject</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    @*<link rel="stylesheet" href="~/css/site.css" />*@
</head>
<body>
    <div contenteditable="false" class="p-3 mb-2 bg-white text-black-50">
        <header>

            @*<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">

                <div class="container">

                    <a class="navbar-brand" asp-area="" asp-controller="HOD" asp-action="Index">InnTune</a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">

                        <ul class="navbar-nav flex-grow-1">
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="HOD" asp-action="Index">Dashboard</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="HOD" asp-action="EnrolUser">Enrol Staff</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="HOD" asp-action="Grade">Grade</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="HOD" asp-action="Department">Department</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="HOD" asp-action="EnrolSubject">Enrol Subject</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="HOD" asp-action="UploadFile">Upload File</a>
                            </li>
                        </ul>
                        <partial name="_LoginPartial" />

                    </div>


                </div>

            </nav>*@
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <div class="container-fluid">
                    <a class="navbar-brand" href="#">Navbar</a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                            <li class="nav-item">
                                <a class="nav-link active" aria-current="page" asp-area="" asp-controller="HOD" asp-action="Index">Home</a>
                            </li>
                            @*<li class="nav-item">
                                <a class="nav-link" href="#">Link</a>
                            </li>*@
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" asp-area="" asp-controller="HOD" asp-action="Index" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Dashboard
                                </a>
                                <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    @*<li><a >Departement</a></li>*@
                                    <li><a class="dropdown-item" asp-area="" asp-controller="HOD" asp-action="EnrolUser">Enrol Staff</a></li>
                                    <li><a class="dropdown-item" asp-area="" asp-controller="HOD" asp-action="Grade">Enrol Grade</a></li>
                                    <li><a class="dropdown-item" asp-area="" asp-controller="HOD" asp-action="Department">Enrol Department</a></li>
                                    <li><a class="dropdown-item" asp-area="" asp-controller="HOD" asp-action="Subject">Enrol Subject</a></li>
                                    <li><a class="dropdown-item" asp-area="" asp-controller="HOD" asp-action="Profile">Profile</a></li>
                                    <li><a class="dropdown-item" asp-area="" asp-controller="HOD" asp-action="Reporting">Departmental Report</a></li>
