@model tobedeleted.Models.Subject

@{
    ViewData["Title"] = "UpdateSubject";
    Layout = "~/Pages/_LayoutHOD.cshtml";
}
<div class="align-text-center">
    <h1>Update Subject</h1>

    <h4>Subject</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="UpdateSubject">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="SubID" />
                <div class="form-group">
                    <label asp-for="SubDesc" class="control-label"></label>
                    <input asp-for="SubDesc" class="form-control" />
                    <span asp-validation-for="SubDesc" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>

    <div>
        <a asp-action="Dashboard">Back to List</a>
    </div>
</div>


